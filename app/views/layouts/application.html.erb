  <title>FoxtrotMike</title>
  <%= csrf_meta_tag %>
  <%= stylesheet_link_tag :all %>
  <%= stylesheet_link_tag 'scaffold' %>
  <%= stylesheet_link_tag 'formtastic' %>
  <%= stylesheet_link_tag 'app' %>
  <%= javascript_include_tag 'jquery-1.4.2' %>
  <%= javascript_include_tag 'i18n' %>
