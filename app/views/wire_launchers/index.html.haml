%h1= t('.title')
%table.filtered{ :"data-filter-by" => "filter_string" }
  %tr.no_filter.show_on_startup
    %th= t("views.filter_search")
  %tr.no_filter.show_on_startup
    %td= text_field_tag "filter_string"
  %tr.no_filter
    %th= WireLauncher.l(:registration)
  - @wire_launchers.each do |wire_launcher|
    %tr.alternate_color{ :"data-filter_string" => wire_launcher.registration.downcase }
      %td= h wire_launcher.registration
      %td= link_to t('views.show'), wire_launcher
      %td= link_to t('views.edit'), edit_wire_launcher_path(wire_launcher)
= link_to t('views.new'), new_wire_launcher_path
